{
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({Environment}/{Application}/{MachineName}/{EnvironmentUserName}) {SourceContext} {Message:lj}{NewLine}{Exception}",
          "path": "D:\\Logs\\msA\\msa.devl.log",
          "pathFormat": "log-{Date}.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithEnvironmentUserName" ],
    "Properties": {
      //"Application": "msone"
    }
  },
  "AllowedHosts": "*"
}